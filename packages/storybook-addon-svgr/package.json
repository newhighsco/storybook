    "@storybook/react-webpack5": "7.6.17",
    "storybook": "7.6.17"
  },
  "storybook": {
    "displayName": "SVGR",
    "supportedFrameworks": [
      "react"
    ],
    "icon": "https://raw.githubusercontent.com/gregberge/svgr/main/website/images/logo-manifest.png"
  }
