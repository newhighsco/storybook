{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "10.2.7",
  "author": "New High Score",
  "license": "ISC",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook.git",
    "directory": "packages/storybook-preset"
  },
  "homepage": "https://github.com/newhighsco/storybook/tree/main/packages/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/preview.d.ts",
      "default": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./package.json": "./package.json",
    "./*.md?raw": "./*.md"
  },
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rm -rf dist",
    "build": "tsup --silent"
  },
  "dependencies": {
    "@newhighsco/chipset": "6.27.3",
    "@newhighsco/storybook-addon-svgr": "4.0.0",
    "@newhighsco/storybook-addon-transpile-modules": "4.0.0",
    "@storybook/addon-a11y": "10.2.8",
    "@storybook/addon-docs": "10.2.8",
    "@storybook/addon-styling-webpack": "3.0.0",
    "@storybook/addon-webpack5-compiler-swc": "4.0.2",
    "css-loader": "7.1.4",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.1",
    "sass": "1.97.3",
    "sass-loader": "16.0.7",
    "storybook-addon-pseudo-states": "10.2.8",
    "style-loader": "4.0.0"
  },
  "peerDependencies": {
    "@storybook/react-webpack5": "^10",
    "storybook": "10.2.8"
  }
}
