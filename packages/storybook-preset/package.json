{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "7.1.7",
  "author": "New High Score",
  "license": "ISC",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook.git",
    "directory": "packages/storybook-preset"
  },
  "homepage": "https://github.com/newhighsco/storybook/tree/main/packages/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preview": {
      "import": "./dist/preview.mjs",
      "require": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./package.json": "./package.json",
    "./*.md?raw": "./*.md"
  },
  "files": [
    "dist",
    "*.js"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rm -rf dist",
    "buildSkip": "babel --root-mode upward src --out-dir dist --ignore '**/__tests__'",
    "build": "tsup --silent"
  },
  "devDependencies": {
    "@newhighsco/ts-config": "1.0.2"
  },
  "dependencies": {
    "@newhighsco/storybook-addon-svgr": "2.0.60",
    "@newhighsco/storybook-addon-transpile-modules": "2.0.60",
    "@storybook/addon-a11y": "9.1.6",
    "@storybook/addon-docs": "9.1.6",
    "@storybook/addon-styling-webpack": "2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "4.0.1",
    "css-loader": "7.1.2",
    "postcss": "8.5.6",
    "postcss-loader": "8.2.0",
    "sass": "1.92.1",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0"
  },
  "peerDependencies": {
    "@storybook/react-webpack5": "9.1.6",
    "storybook": "9.1.6"
  }
}
