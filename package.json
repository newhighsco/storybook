{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "3.1.4",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "start-storybook --port 6006 --docs --ci --quiet",
    "prepare": "npm run build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.js'",
    "build:storybook": "build-storybook --output-dir build --docs --quiet",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "npm run test:js -- --watch",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-docs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@svgr/webpack": "5.4.0",
    "react-is": "17.0.1",
    "sass": "1.29.0",
    "sass-loader": "10.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/polyfill": "7.12.1",
    "@commitlint/cli": "11.0.0",
    "@newhighsco/commitlint-config": "1.0.7",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.48",
    "@newhighsco/prettier-config": "2.0.6",
    "@newhighsco/release-config": "1.0.34",
    "@storybook/react": "6.0.28",
    "babel-jest": "26.6.3",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "semantic-release": "17.2.2"
  },
  "peerDependencies": {
    "@storybook/react": "6.0.28"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:storybook"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
