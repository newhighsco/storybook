{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "2.3.10",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "start": "start-storybook --port 6006 --docs --ci --quiet",
    "prepare": "npm run build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.js'",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-scss": "1.0.2",
    "@svgr/webpack": "5.4.0",
    "react-is": "16.13.1",
    "sass": "1.26.10",
    "sass-loader": "9.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/polyfill": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@newhighsco/commitlint-config": "1.0.3",
    "@newhighsco/editor-config": "1.1.1",
    "@newhighsco/eslint-config": "2.2.11",
    "@newhighsco/prettier-config": "2.0.3",
    "@newhighsco/release-config": "1.0.26",
    "@storybook/react": "5.3.19",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.5.0",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "semantic-release": "17.1.1"
  },
  "peerDependencies": {
    "@storybook/react": "5.3.19"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config"
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ]
  }
}
