{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "4.5.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "preset.js"
  ],
  "scripts": {
    "prestart": "yarn build",
    "start": "start-storybook --port 6006 --docs --no-manager-cache",
    "prepare": "(is-ci || husky install) && yarn build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.jsx'",
    "prebuild:storybook": "rm -rf build",
    "build:storybook": "build-storybook --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore --ext .js,.json,.jsx,.mdx .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "6.4.20",
    "@storybook/addon-backgrounds": "6.4.20",
    "@storybook/addon-docs": "6.4.20",
    "@storybook/addon-links": "6.4.20",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.4.20",
    "@storybook/preset-scss": "1.0.3",
    "@svgr/webpack": "6.2.1",
    "new-url-loader": "0.1.1",
    "postcss": "8.4.12",
    "react-is": "18.0.0",
    "sass": "1.49.11",
    "sass-loader": "12.6.0",
    "storybook-dark-mode": "1.0.9"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.11",
    "@commitlint/cli": "16.2.3",
    "@newhighsco/commitlint-config": "1.1.0",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "3.2.2",
    "@newhighsco/prettier-config": "2.1.1",
    "@newhighsco/release-config": "1.3.4",
    "@storybook/builder-webpack5": "6.4.20",
    "@storybook/manager-webpack5": "6.4.20",
    "@storybook/react": "6.4.20",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.7",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.4",
    "chromatic": "6.5.3",
    "enzyme": "3.11.0",
    "eslint": "8.12.0",
    "husky": "7.0.4",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "semantic-release": "19.0.2",
    "webpack": "5.71.0"
  },
  "peerDependencies": {
    "@storybook/react": "6.4.20"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:storybook"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
