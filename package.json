{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "4.5.50",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "preset.js"
  ],
  "scripts": {
    "prestart": "yarn build",
    "start": "start-storybook --port 6006 --docs --no-manager-cache",
    "prepare": "(is-ci || husky install) && yarn build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.jsx'",
    "prebuild:storybook": "rm -rf build",
    "build:storybook": "build-storybook --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-backgrounds": "6.5.13",
    "@storybook/addon-docs": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.5.13",
    "@storybook/preset-scss": "1.0.3",
    "@svgr/webpack": "6.5.1",
    "new-url-loader": "0.1.1",
    "postcss": "8.4.19",
    "react-is": "18.2.0",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "storybook-dark-mode": "1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.20.2",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@commitlint/cli": "17.2.0",
    "@newhighsco/commitlint-config": "1.1.9",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "3.4.11",
    "@newhighsco/prettier-config": "2.1.3",
    "@newhighsco/release-config": "1.3.9",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@wojtekmaj/enzyme-adapter-react-17": "0.8.0",
    "babel-jest": "27.5.1",
    "babel-loader": "9.1.0",
    "chromatic": "6.11.4",
    "enzyme": "3.11.0",
    "eslint": "8.28.0",
    "husky": "8.0.2",
    "is-ci": "3.0.1",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "19.0.5",
    "webpack": "5.75.0"
  },
  "peerDependencies": {
    "@storybook/react": "6.5.13"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:storybook"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
