{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "5.1.53",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "preset.js"
  ],
  "scripts": {
    "prestart": "yarn build",
    "start": "storybook dev --port 6006 --docs",
    "prepare": "(is-ci || husky install) && yarn build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.jsx'",
    "prebuild:storybook": "yarn build",
    "build:storybook": "storybook build --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ignore-path .gitignore .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "7.3.2",
    "@storybook/addon-backgrounds": "7.3.2",
    "@storybook/addon-docs": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "7.3.2",
    "@storybook/preset-scss": "1.0.3",
    "@svgr/webpack": "8.1.0",
    "new-url-loader": "0.1.1",
    "postcss": "8.4.28",
    "react-is": "18.2.0",
    "sass": "1.66.1",
    "sass-loader": "13.3.2",
    "storybook-dark-mode": "3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.22.10",
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@commitlint/cli": "17.7.1",
    "@newhighsco/commitlint-config": "1.1.19",
    "@newhighsco/editor-config": "1.2.0",
    "@newhighsco/eslint-config": "4.0.60",
    "@newhighsco/prettier-config": "2.1.14",
    "@newhighsco/release-config": "1.3.24",
    "@storybook/react-webpack5": "7.3.2",
    "chromatic": "6.23.0",
    "eslint": "8.47.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "21.1.0",
    "storybook": "7.3.2",
    "webpack": "5.88.2"
  },
  "peerDependencies": {
    "@storybook/react-webpack5": "7.3.2",
    "storybook": "7.3.2"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config"
    ]
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:storybook"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
