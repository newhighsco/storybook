{
  "name": "@newhighsco/storybook-preset",
  "description": "New High Score shareable Storybook preset",
  "version": "4.3.24",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/storybook-preset.git"
  },
  "homepage": "https://github.com/newhighsco/storybook-preset#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/storybook-preset/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "preset.js"
  ],
  "scripts": {
    "prestart": "yarn prepare",
    "start": "start-storybook --port 6006 --docs --no-manager-cache",
    "prepare": "yarn build",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist --ignore '**/__mocks__','**/__tests__','**/*.spec.js'",
    "prebuild:storybook": "rm -rf build",
    "build:storybook": "build-storybook --output-dir build --docs --quiet",
    "pretest": "yarn lint",
    "test": "npm-run-all --parallel test:*",
    "test:js": "jest --coverage",
    "tdd": "yarn test:js --watch",
    "snapshot": "npm-run-all --parallel snapshot:*",
    "snapshot:chromatic": "chromatic --storybook-build-dir build --auto-accept-changes main --exit-once-uploaded",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.json,.jsx,.mdx .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "yarn lint:js --fix"
  },
  "dependencies": {
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-backgrounds": "6.3.12",
    "@storybook/addon-docs": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addons": "6.3.12",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/preset-scss": "1.0.3",
    "@svgr/webpack": "5.5.0",
    "new-url-loader": "0.1.1",
    "postcss": "8.3.11",
    "react-is": "17.0.2",
    "sass": "1.43.4",
    "sass-loader": "12.2.0",
    "storybook-dark-mode": "1.0.8"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.15.8",
    "@commitlint/cli": "13.2.1",
    "@newhighsco/commitlint-config": "1.0.16",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "2.3.36",
    "@newhighsco/prettier-config": "2.0.14",
    "@newhighsco/release-config": "1.0.73",
    "@storybook/react": "6.3.12",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "chromatic": "6.0.4",
    "enzyme": "3.11.0",
    "eslint": "8.1.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "semantic-release": "18.0.0",
    "webpack": "5.60.0"
  },
  "peerDependencies": {
    "@storybook/react": "6.3.12"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco/eslint-config/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ],
    "packageRules": [
      {
        "extends": [
          "monorepo:storybook"
        ],
        "semanticCommitType": "fix"
      }
    ]
  }
}
